{
  "name": "@c9d/config",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "env-wrapper eslint src/",
    "test": "NODE_OPTIONS=\"--max-old-space-size=8192\" env-wrapper vitest run",
    "test:run": "NODE_OPTIONS=\"--max-old-space-size=8192\" env-wrapper vitest run",
    "test:dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" env-wrapper vitest --watch",
    "test:watch": "NODE_OPTIONS=\"--max-old-space-size=8192\" env-wrapper vitest --watch",
    "typecheck": "env-wrapper tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@phase.dev/phase-node": "^3.1.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "phase": {
    "appName": "AI.C9d"
  }
}