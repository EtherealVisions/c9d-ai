{
  "name": "@c9d/config",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "edge-light": "./dist/index.browser.js",
      "worker": "./dist/index.browser.js",
      "browser": "./dist/index.browser.js",
      "node": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.browser.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:node && pnpm build:browser",
    "build:node": "tsc --project tsconfig.json",
    "build:browser": "tsc --project tsconfig.browser.json",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "test": "node ../../scripts/run-with-env.js vitest run",
    "test:run": "node ../../scripts/run-with-env.js vitest run",
    "test:dev": "node ../../scripts/run-with-env.js vitest --watch",
    "test:watch": "node ../../scripts/run-with-env.js vitest --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@phase.dev/phase-node": "^3.1.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "phase": {
    "appName": "AI.C9d.Web"
  }
}