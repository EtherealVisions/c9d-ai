{
  "$schema": "../env-tools/schemas/env-config-schema.json",
  "appName": "AI.C9d",
  "displayName": "C9D UI Components",
  "description": "Shared UI component library for the C9D AI platform",
  "envVars": {
    "required": [],
    "optional": [
      {
        "name": "NODE_ENV",
        "description": "Node.js environment",
        "type": "string",
        "enum": ["development", "production", "test"],
        "default": "development"
      },
      {
        "name": "STORYBOOK_PORT",
        "description": "Storybook development server port",
        "type": "number",
        "default": 6006,
        "validation": {
          "min": 1000,
          "max": 65535
        }
      }
    ]
  },
  "validation": {
    "strict": false,
    "warnOnMissing": false,
    "failOnInvalid": false
  }
}