{
  "name": "@c9d/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:dev": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run **/*.test.{ts,tsx}",
    "test:integration": "vitest run __tests__/integration/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:performance": "vitest run __tests__/performance/**/*.test.{ts,tsx}",
    "test:comprehensive": "tsx __tests__/test-runner.ts",
    "test:mocked": "npx vitest run __tests__/integration/auth-flow-fixed.integration.test.ts",
    "test:real-integration": "tsx __tests__/run-real-integration-tests.ts",
    "test:real-db": "npx vitest run __tests__/integration/real-database-integration.test.ts",
    "test:real-clerk": "npx vitest run __tests__/integration/real-clerk-integration.test.ts",
    "test:setup": "tsx __tests__/setup-real-integration-tests.ts",
    "test:validate": "tsx __tests__/validate-real-integrations.ts",
    "test:coverage": "vitest run --coverage",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:coverage": "COVERAGE_E2E=true playwright test",
    "test:watch": "vitest --watch",
    "coverage:open": "open coverage/index.html",
    "coverage:json": "vitest run --coverage --reporter=json",
    "coverage:lcov": "vitest run --coverage --reporter=lcov",
    "coverage:report": "node ../../scripts/coverage-reporter.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate:quick": "pnpm typecheck && pnpm lint",
    "validate:full": "pnpm typecheck && pnpm lint && pnpm test:coverage && pnpm build",
    "validate:coverage": "pnpm test:coverage && pnpm coverage:report",
    "validate:task-completion": "node ../../scripts/validate-task-completion.js"
  },
  "dependencies": {
    "@c9d/config": "workspace:*",
    "@c9d/types": "workspace:*",
    "@c9d/ui": "workspace:*",
    "@clerk/nextjs": "^6.31.10",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "1.2.2",
